{
  "extends": ["next", "prettier"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "off",
    "@next/next/no-img-element": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "no-const-assign": "error",
    "no-var": "error"
  },
  "ignorePatterns": ["node_modules/", ".next/", "out/", "src/prisma/client"]
}
